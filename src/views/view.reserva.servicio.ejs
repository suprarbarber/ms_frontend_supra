<%- include('partials/headerhtml') %>
    
    <link rel="stylesheet" href="./css/reserva.servicio.css" />
    <title>Reserva Servicios</title>
</head>

<body>
  <div class="main-container d-flex">
    <!-- barra lateral  -->
     <%- include('partials/sidebar') %>
     
    <div class="content">

        <!-- barra de navegacion superior  -->
        <%- include('partials/navbar.ejs') %>
        <!-- fin barra superior  -->

        <!-- contenido de la pagina  -->
        <div class="container-fluid text-center" id="content">
            <div class="row">
                <h3 class="title">Selecciona el servicio deseado</h3>
            </div>
            <div class="row mt-2">
                <div class="col-md-12 center" id="cuerpo">
                    <% datos.forEach(element => { %>
                        <button onclick="rutaCorte('<%= element.id_servicio %>')"  id="btn-servicio" type="button" class="btn btn-outline-success fs-5 btn-servicio mx-2 my-3"><%= element.nombre %></button>
                    <% }); %>
                </div>
            </div>
        </div>
            
    </div>
  

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="./js/script.barra.lateral.js"></script>
    <script src="../js/reserva.servicio.js"></script>
</body>
</html>